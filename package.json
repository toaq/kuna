{
	"devDependencies": {
		"@types/jest": "^29.2.5",
		"esbuild-runner": "^2.2.2",
		"jest": "^29.3.1",
		"prettier": "^3.0.0",
		"ts-jest": "^29.0.3",
		"typescript": "^4.9.4"
	},
	"name": "kuna",
	"version": "0.0.1",
	"description": "Tools for parsing Toaq Delta",
	"main": "index.js",
	"scripts": {
		"test": "echo \"Error: no tests configured\" && exit 1",
		"prettier:check": "npx -p prettier prettier -c .",
		"prettier:format": "npx -p prettier prettier -w .",
		"cli": "npx esr src/index.ts",
		"cli:debug": "node --inspect-brk -r esbuild-runner/register src/index.ts",
		"bot": "npx esr src/bot.ts",
		"bot:debug": "node --inspect-brk -r esbuild-runner/register src/bot.ts",
		"codegen": "npx nearleyc src/toaq.ne -o src/grammar.ts",
		"ts:build": "npx tsc",
		"ts:check": "npx tsc --noEmit"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/toaq/kuna.git"
	},
	"author": "Lynn",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/toaq/kuna/issues"
	},
	"homepage": "https://github.com/toaq/kuna#readme",
	"dependencies": {
		"@types/nearley": "^2.11.2",
		"@types/yargs": "^17.0.20",
		"canvas": "^2.11.0",
		"discord.js": "^14.7.1",
		"nearley": "^2.20.1",
		"yargs": "^17.6.2"
	},
	"jest": {
		"testRegex": "build/index.js",
		"verbose": true,
		"testEnvironment": "node"
	}
}
